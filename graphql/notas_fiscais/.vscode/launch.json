{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Uvicorn",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "main:app",
        "--reload",
        "--lifespan", "on",
        "--host", "0.0.0.0",
        "--port", "8080"
